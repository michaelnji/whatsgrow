		"dotenv": "^16.0.1",
		"felte": "^1.2.2",
		"file-saver": "^2.0.5",
		"frappe-charts": "^1.6.2",
		"svelte-feather-icons": "^4.0.0",
		"svelte-heros": "^2.2.3",
		"uuid": "^8.3.2",
		"vcards-js": "^2.10.0"
	},
	"type": "module"
}
